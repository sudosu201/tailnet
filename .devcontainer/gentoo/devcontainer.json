{
  "name": "Gentoo",
  "service": "tailnet",
  "containerEnv": {
    "BOOT": "gentoo",
    "CPU_CORES": "max",
    "RAM_SIZE": "half",
    "DISK_SIZE": "174G",
    "MACHINE": "q35",
    "KVM": "Y",
    "GPU": "Y",
    "DISK_FMT": "qcow2",
    "DISK_TYPE": "scsi",
    "DISK_IO": "threads",
    "VM_NET_IP": "10.4.20.99",
    "ENGINE": "podman",
    "SUPPORT": "https://github.com/sudosu201/tailnet"
  },
  "forwardPorts": [
    8006
  ],
  "portsAttributes": {
    "8006": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "dockerComposeFile": "../codespaces.yml",
  "initializeCommand": "podman  system prune --all --force"
}